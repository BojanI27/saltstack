server_host     = {{ dcip }}
server_port     = 389
version         = 3
bind            = yes
start_tls       = no
bind_dn         = {{ query_user }}
bind_pw         = {{ query_password }}
search_base     = {{ search_base }}
scope           = sub
query_filter    = (&(userPrincipalName=%s)(objectClass=person)(!(userAccountControl:1.2.840.113556.1.4.803:=2)))
result_attribute= userPrincipalName
debuglevel      = 0
